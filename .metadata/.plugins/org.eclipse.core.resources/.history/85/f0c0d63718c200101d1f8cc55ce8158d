package com.chubb.request;

import org.junit.jupiter.api.Test;
import org.junit.jupiter.api.Assertions;

public class Order1Test {

	@Test
	void testOrderProperties() {
		Order1 order = new Order1();
		order.setItem("Test Item");
		order.setPrice(1000);
		order.setQuantity(2);
		
		Assertions.assertEquals("Test Item", order.getItem());
		Assertions.assertEquals(100, order.getPrice());
		Assertions.assertEquals(2, order.getQuantity());
		
	}
}
